+++ b/board-base.mk
@@ -4,3 +4,4 @@ TARGET_EXFAT_DRIVER := exfat
 DEVICE_FRAMEWORK_MANIFEST_FILE := device/phh/treble/framework_manifest.xml

 BOARD_ROOT_EXTRA_FOLDERS += bt_firmware sec_storage efs
